     <script type="text/javascript">
      $(document).ready(function(){
      $(window).scroll(function(){
        var scroll = $(window).scrollTop();
        if (scroll > 70) {
          $(".navbar-default").css("background" , "#46188d");
        }

          else{
            $(".navbar-default").css("background" , "rgba(25, 24, 24, 0.89)");   
          }
        })
      })
    </script>
    
